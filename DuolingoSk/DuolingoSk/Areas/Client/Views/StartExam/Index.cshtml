<script>
    var cntintr = 0;
    $(document).ready(function () {
        $("#bdt").val('@bdd');
        $("#mdt").val('@bdm');
        $("#ydt").val('@bdy');
        $(".question").hide();
    });
    function Start() {
        $.blockUI();
        $("#dvStartNow").load('\startnow?LevelId=' + $("#quelevel").val(), function () {
            $(".question").hide();
            $(".indexpage").hide();
            var qiid = questionss[cntQs].QuestionId
            $(".container1").load('\getQuestion?QueId=' + qiid, function () {
                $.unblockUI();
            });
        });
        //$(".1q").show();
    }

    function nextintro() {
        if (cntintr > 0 && $("#quelevel").val() == "") {
            alert("Please Select Level");
            return false;
        }
        else {
            $(".intr" + cntintr).hide();
            cntintr = cntintr + 1;
            $(".intr" + cntintr).show();
        }
    }

    function backintro() {
        if ($("#quelevel").val() == "") {
            alert("Please Select Level");
            return false;
        }
        else {
            $(".intr" + cntintr).hide();
            cntintr = cntintr - 1;
            $(".intr" + cntintr).show();
        }
    }

    function checklevl() {
        var qlv = "0";
        if ($("#quelevel").val() != "") {
            qlv = $("#quelevel").val();
        }
        var URL = '/Client/StartExam/CheckLevel?LevelId=' + qlv;
        $.ajax({
            type: 'POST',
            async: false,
            url: URL,
            success: function (result) {
                if (result == "Success") {
                    nextintro();
                }
                else {
                    alert(result);
                }
            },
            error: function (resultData) {
                console.log("error");
                return false;
            }
        });
    }
</script>
