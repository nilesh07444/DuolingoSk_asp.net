    }

    function startimes() {
        var tmr2 = setInterval(function () {
            if (tmmmrr == 0) {
                clearInterval(tmr2);        
                $(".uplds").html("Upload complete");
            }
            else {
                tmmmrr = tmmmrr - 1;            
                $('progress').attr('value', tmmmrr);
            }
        }, 150);
    }  
</script>
